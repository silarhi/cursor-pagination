    "type": "library",
    "require": {
        "php": "~8.0.0 || ~8.1.0 || ~8.2.0 || ~8.3.0",
        "doctrine/orm": "^2.5"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.48",
        "phpunit/phpunit": "^9.6.15",
        "phpstan/phpstan": "^1.10.57",
        "rector/rector": "^0.19",
        "vimeo/psalm": "^5.20",
        "doctrine/data-fixtures": "^1.7",
        "symfony/cache": "^7.0.3"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Silarhi\\CursorPagination\\": "src/"
        }
    },
    "autoload-dev": {
