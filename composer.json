{
    "name": "silarhi/cursor-pagination",
    "description": "A doctrine ORM cursor based pagination library for faster batch operations.",
    "keywords": ["doctrine", "orm", "pagination", "cursor", "cursor-pagination"],
    "type": "library",
    "require": {
        "php": ">=8.0",
        "doctrine/orm": "^3.1.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.52",
        "phpunit/phpunit": "^9.6.15",
        "phpstan/phpstan": "^1.10.65",
        "rector/rector": "^1.0",
        "vimeo/psalm": "^5.23",
        "doctrine/data-fixtures": "^1.7",
        "symfony/cache": "^6.0|^7.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Silarhi\\CursorPagination\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Silarhi\\CursorPagination\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "SILARHI",
            "email": "dev@silarhi.fr"
        }
    ]
}
